{
  "name": "sine-qua-non",
  "description": "A Webpack boilerplate for modern Javascript development",
  "version": "0.0.0-b.1.0.0",
  "author": {
    "name": "Thomas Bishop",
    "url": "https://www.thomas-bishop.co.uk"
  },
  "license": "MIT",
  "keywords": [
    "html5",
    "css3",
    "es6",
    "boilerplate",
    "webpack"
  ],
  "scripts": {
    "prestart": "gulp htmlConcat",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js",
    "start:dist": "cross-env NODE_ENV=production webpack-dev-server --config ./config/webpack.config.js && http-server ./dist -o",
    "setup": "node ./config/site.setup.js",
    "lint:js": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "postbuild": "gulp sfm",
    "deploy": "node ./config/site.deploy.js",
    "exec-webp": "node config/makeWebpImages"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "bootstrap": "^5.0.0-alpha2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.1",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "del": "^6.0.0",
    "ejs-loader": "^0.5.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.22.1",
    "extract-loader": "^5.1.0",
    "file-loader": "^4.3.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-inject-partials": "^1.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-source-injector": "^1.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-inline-svg-plugin": "^2.3.0",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "^0.12.3",
    "image-webpack-loader": "^5.1.0",
    "imagemin": "^7.0.1",
    "imagemin-webp": "^6.0.0",
    "merge-stream": "^2.0.0",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "popper.js": "^1.16.1",
    "postcss-loader": "^3.0.0",
    "postcss-purgecss": "^2.0.3",
    "postcss-sass": "^0.4.4",
    "robotstxt-webpack-plugin": "^6.0.0",
    "sass-loader": "^7.3.1",
    "sitemap-webpack-plugin": "^0.6.0",
    "style-loader": "^1.3.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webapp-webpack-plugin": "^2.7.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2",
    "webpackbar": "^4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "ftp-deploy": "^2.3.8",
    "sanitize.css": "^11.0.1",
    "webpack-dev-server": "^3.11.0"
  }
}
